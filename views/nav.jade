- var classes = ['navbar', 'navbar-custom','navbar-fixed-top']
- var rightNavClasses = ['collapse','navbar-collapse','navbar-right','navbar-main-collapse']

nav(class=classes)&attributes({'role':'navigation'})
    div.container
        div.navbar-header
            button.navbar-toggle(type='button')&attributes({'data-toggle':'collapse', 'data-target':'.navbar-main-collapse'})
                i.fa.fa-bars
            a.navbar-brand.page-scroll(href='#page-top')
                span ELENA BOYD
        div(class=rightNavClasses)
            ul.nav.navbar-nav
                li.hidden
                    a(href="#page-top")
                li
                    a.page-scroll(href="#about") about
                li
                    a.page-scroll(href="#work") work
                li
                    a.page-scroll('href'='#contact') contact